
public class PonyExpress {
	public static int jinetes(int[] distancias) {
		int acumulador = 0;
		int cantidadJinetes = 0;
		for (int distancia : distancias) {
			acumulador+= distancia;
			if(acumulador > 100) {
				cantidadJinetes++;
				acumulador = 0;
			}
		}
		return cantidadJinetes;
	}
	
	public static void main(String[] args) {
		PonyExpress jinete;
		
		System.out.println(jinetes(new int[] {33, 8, 16, 47, 30, 30, 46}));
	}

}
