
public class PonyExpress {
	public static int jinetes(int[] distancias) {
		int acumulador = 100;
		int cantidadJinetes = 0;
		for (int distancia : distancias) {
			acumulador-= distancia;
			if(acumulador < 0) {
				cantidadJinetes++;
				acumulador = 100;
			}
		}
		return cantidadJinetes;
	}
	
	public static void main(String[] args) {
		PonyExpress jinete;
		
		System.out.println(jinetes(new int[] {51,51,51}));
	}

}
