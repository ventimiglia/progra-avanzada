import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class CirculoTest {

	@Test
	void test() {
		Circulo circle1 = new Circulo(new Punto(2, 2), 2);
		Circulo circle2 = new Circulo(new Punto(6, 4), 2);
		
		circle1.intersectaCon(circle2);
		assertEquals(1, 1);
	}

}
