
public class Punto {
	private double x;
	private double y;
	
	public Punto(double _x, double _y) {
		this.x = _x;
		this.y = _y;
	}
	
	double getCordenadaX () {
		return this.x;
	}
	
	double getCordenadaY () {
		return this.y;
	}

	public static void main(String[] args) {
		Punto puntoCero = new Punto(0,0);
		System.out.println(puntoCero);
	}
}
